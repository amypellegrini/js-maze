{
  "name": "js-maze",
  "version": "1.0.0",
  "description": "A JavaScript maze generator.",
  "main": "index.js",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "prepare": "npm run build"
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "author": "Amy Pellegrini <amyvpellegrini@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amypellegrini/js-maze.git"
  },
  "keywords": [
    "maze",
    "js",
    "ts"
  ],
  "bugs": {
    "url": "https://github.com/amypellegrini/js-maze/issues"
  },
  "homepage": "https://github.com/amypellegrini/js-maze#readme",
  "devDependencies": {
    "semantic-release": "^21.1.1",
    "typescript": "^5.2.2",
    "vitest": "^0.34.3"
  }
}
